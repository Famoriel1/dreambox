    function onWindowResizedJS() {
        resizeHelper();
    }

    var bodyScale, bodyWidth, bodyHeight;
    
    function resizeHelper() {
        var w = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth; // || stuff for IE 7 and 8 support
        var h = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
        var wScale = w/1024;
        var hScale = h/788;
        var scale;
        if (wScale < hScale) {
            // width-constrained
            scale = wScale;
        } 
        else {
            // height-constrained
            scale = hScale;
        }
        // minimum size
        scale = Math.max(scale, 0.25);
        var wScaled = Math.ceil(1024*scale);
        var hScaled = Math.ceil(768*scale);
        var x = Math.floor((w - wScaled) / 2);
        
        var pixelRatio = window.devicePixelRatio >= 2 ? 2 : 1;

        var backgroundCanvas = document.getElementById("lrt4BackgroundCanvas");
        if (backgroundCanvas) {
            backgroundCanvas.width = wScaled * pixelRatio;
            backgroundCanvas.height = hScaled * pixelRatio;
            backgroundCanvas.style.width = wScaled+"px";
            backgroundCanvas.style.height = hScaled+"px";
            backgroundCanvas.style.left = x + "px";
        }
        var spriteCanvas = document.getElementById("lrt4SpriteCanvas");
        if (spriteCanvas) {
            spriteCanvas.width = wScaled * pixelRatio;
            spriteCanvas.height = hScaled * pixelRatio;
            spriteCanvas.style.width = wScaled+"px";
            spriteCanvas.style.height = hScaled+"px";
            spriteCanvas.style.left = x + "px";
        }

        var lessonCanvas = document.getElementById("lrt4LessonCanvas");
        lessonCanvas.width = wScaled * pixelRatio;
        lessonCanvas.height = hScaled * pixelRatio;
        lessonCanvas.style.width = wScaled+"px";
        lessonCanvas.style.height = hScaled+"px";
        lessonCanvas.style.left = x + "px";
        var htmlLessonDiv = document.getElementById("lrt4LessonDiv");
        htmlLessonDiv.style.scale = scale.toString();
        htmlLessonDiv.style.left = x + "px";
        htmlLessonDiv.style.top = "0px";
        var engagementCanvas = document.getElementById("lrt4EngagementCanvas");
        engagementCanvas.width = wScaled * pixelRatio;
        engagementCanvas.height = hScaled * pixelRatio;
        engagementCanvas.style.width = wScaled+"px";
        engagementCanvas.style.height = hScaled+"px";
        engagementCanvas.style.left = x + "px";
        var htmlcontentdiv = document.getElementById("htmlcontent");
        htmlcontentdiv.width = wScaled;
        htmlcontentdiv.height = hScaled;
        htmlcontentdiv.style.width = wScaled+"px";
        htmlcontentdiv.style.height = hScaled+"px";
        htmlcontentdiv.style.left = x + "px";
        var copyrightdiv = document.getElementById("copyright");
        copyrightdiv.width = wScaled;
        copyrightdiv.height = hScaled;
        copyrightdiv.style.width = wScaled+"px";
        copyrightdiv.style.height = hScaled+"px";
        copyrightdiv.style.top = hScaled + "px";
        copyrightdiv.style.left = x + "px";
        var jscontentdiv = document.getElementById("jscontent");
        jscontentdiv.width = wScaled;
        jscontentdiv.height = hScaled;
        jscontentdiv.style.width = wScaled+"px";
        jscontentdiv.style.height = hScaled+"px";

        call("runtime", "call", "resize", []); // may be a no-op if runtime not loaded
    }
    </script>
  </body>

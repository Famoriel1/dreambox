<script type="text/javascript">

function closeWindow() {
  removeUnloadBindings();
  window.location.href = "/student/dblexit";
}

function closeWindowIOError(code, suffix) {
  closeWindowErrorUrl("/play/ioerrors", code, suffix);
}

function closeWindowFatalError(code, suffix) {
  closeWindowErrorUrl("/play/fatalerrors", code, suffix);
}

function closeWindowSetupParent() {
  removeUnloadBindings();
  window.location.href = "https://play.dreambox.com/dashboard/home/setup_parent_access";
}

function closeWindowRedirect(url) {
  removeUnloadBindings();
  window.location.href = url;
}

function closeWindowErrorUrl(url, code, suffix) {
  var params = {}, params_ct = 0;
  removeUnloadBindings();
  if (!!code) {
    params['code'] = code;
    params_ct++;
  }
  if (!!suffix) {
    params['suffix'] = suffix;
    params_ct++;
  }
  if (params_ct > 0) {
    url += '?' + $.param(params);
  }
  window.location.href = url;
}

function removeUnloadBindings() {
  $(window).unbind('beforeunload');
  removeFinalUnloadCalls();
}

function removeFinalUnloadCalls() {
  $(window).unbind('unload');
}

function addFinalUnloadCall(settings /* object */) {
  if(!settings || !settings.url) return;
  if(!settings.dataType) settings.dataType = "text";
  settings.headers = { "DBL-ClientInstanceId": "6c5a455b-3f1c-4a92-b599-54e7653185b2" };
  $(window).bind('unload',function(e) {
    $.ajax(settings);
  });
}

$(window).bind('beforeunload', function(e) {
});

  // -----------------------------------------------------------------------------
  // Ask the user if he/she really wants to close the page
  $(window).bind('beforeunload', function(e) {
    return "If you are playing a lesson, your progress will be lost."
  });

</script>
